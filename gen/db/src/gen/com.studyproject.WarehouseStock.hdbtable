COLUMN TABLE com_studyproject_WarehouseStock (
  StockId NVARCHAR(36) NOT NULL,
  Product_ProductId NVARCHAR(36),
  Category NVARCHAR(50),
  CurrentStock INTEGER,
  Location NVARCHAR(100),
  PRIMARY KEY(StockId)
) WITH ASSOCIATIONS (
  MANY TO ONE JOIN com_studyproject_Products AS "PRODUCT" ON ("PRODUCT".ProductId = Product_ProductId)
)