COLUMN TABLE com_studyproject_Products (
  ProductId NVARCHAR(36) NOT NULL,
  ProductName NVARCHAR(100),
  Category NVARCHAR(50),
  Manufacturer NVARCHAR(100),
  Quantity INTEGER,
  Price DECIMAL(10, 2),
  Currency NVARCHAR(3),
  Color NVARCHAR(50),
  Location NVARCHAR(100),
  Description NVARCHAR(500),
  PRIMARY KEY(ProductId)
) WITH ASSOCIATIONS (
  MANY TO MANY JOIN com_studyproject_Items AS OrderItems ON (OrderItems.Product_ProductId = ProductId)
)